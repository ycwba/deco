generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Decoration {
  id        Int      @id @default(autoincrement())
  x         String
  y         String
  icon      String
  nickname  String?
  content   String
  isPrivate Boolean  @default(false)
  images    String?
  createdAt DateTime @default(now())
}